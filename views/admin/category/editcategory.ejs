<%- include('../../header.ejs') %>

<!-- ***********sidebar********** -->
<section class="dashboard-wrap mtb-40">
  <div class="container">
    <div class="body-content">
      <div class="row">
        <div class="col-md-3">
          <%- include('../../admin/sidebar.ejs') %>
        </div>
<!-- ************blog update edit*********** -->
        <div class="col-md-9">
          <div class="dash-right">
            <div class="dash-header">
              <div class="dash-title">
                <h1>Category Update</h1>
              </div>
            </div>
          </div>
          <form action="/categoryupdate/<%= edit.id %>" method="post" enctype="multipart/form-data">
            <div class="mb-3">
                <label for="">Catname</label>
                <input type="text" class="form-control" name="catname" value="<%= edit.catname %>">
            </div>
            <button type="Submit" class="btn btn-primary">Update</button>
          </form>
        </div>

        <!-- ******remaining part of sidebar******** -->
      </div>
    </div>
  </div>
</section>

