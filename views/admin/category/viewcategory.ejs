<%- include('../../header.ejs') %>

<section class="dashboard-wrap mtb-40">
    <div class="container">
        <div class="body-content">
            <div class="row">
                <div class="col-md-3">
                    <%- include('../../admin/sidebar.ejs') %> 
                </div>
                <div class="col-md-9">
                    <div class="dash-right">
                        <div class="dash-header">
                            <div class="dash-title">
                                <h1>View</h1>
                            </div>
                            <button type="button" class="btn btn-primary" data-bs-toggle="modal" data-bs-target="#exampleModal">
                                Add Category
                              </button>
                        </div>
                        <table class="table table-bordered table-striped">
                            <tr>
                                <th>#ID</th>
                                <th>Catname</th>             
                            </tr>         
                                <tr>
                                    <td><%= view._id %></td>
                                    <td><%= view.catname %></td>                                 
                                </tr>
                        </table>
                    </div>   
                </div>
            </div>
        </div>
    </div>
</section>
<%- include('../../footer.ejs') %>